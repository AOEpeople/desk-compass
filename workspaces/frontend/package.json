{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "vite",
    "format": "eslint --cache --fix .",
    "lint": "eslint . --ext .ts,.svelte",
    "pretest:lint": "yarn lint",
    "pretest:prettier": "prettier --write \"src/**/*.{ts,svelte,css}\"",
    "pretest:svelte": "yarn run check",
    "serve": "vite preview",
    "test": "vitest --no-watch",
    "test:watch": "vitest",
    "test:coverage": "vitest run --no-watch --coverage"
  },
  "devDependencies": {
    "@iconify/svelte": "3.1.6",
    "@sveltejs/vite-plugin-svelte": "3.0.1",
    "@tailwindcss/forms": "0.5.7",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/svelte": "4.0.5",
    "@tsconfig/svelte": "5.0.2",
    "@types/leaflet": "1.9.8",
    "@types/svelte-range-slider-pips": "2.0.4",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "@vitest/coverage-v8": "*",
    "@vitest/ui": "1.6.0",
    "autoprefixer": "10.4.20",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-svelte": "2.35.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest-environment-jsdom": "29.7.0",
    "jsdom": "23.2.0",
    "lint-staged": "15.3.0",
    "msw": "2.3.5",
    "postcss": "8.4.49",
    "postcss-import": "15.1.0",
    "postcss-load-config": "5.0.2",
    "prettier": "*",
    "prettier-plugin-svelte": "3.1.2",
    "svelte": "4.2.19",
    "svelte-check": "3.6.3",
    "svelte-eslint-parser": "0.33.1",
    "svelte-htm": "1.2.0",
    "svelte-i18n": "4.0.0",
    "svelte-preprocess": "5.1.4",
    "svelte-range-slider-pips": "2.2.3",
    "tailwindcss": "3.4.17",
    "tslib": "2.6.2",
    "typescript": "*",
    "v8": "*",
    "vite": "*",
    "vitest": "*",
    "vitest-dom": "0.1.1"
  },
  "dependencies": {
    "@svelteuidev/composables": "0.15.7",
    "cross-fetch": "4.0.0",
    "leaflet": "1.9.4",
    "svelte-spa-router": "4.0.1"
  },
  "lint-staged": {
    "src/**/*.{ts,svelte,css}": [
      "svelte-check --tsconfig ./tsconfig.json",
      "prettier --write 'src/**/*.{ts,svelte,css}'",
      "eslint --fix --quiet . --ext .ts,.svelte"
    ],
    "*.{yaml,yml,json,js}": [
      "prettier --write"
    ]
  }
}
